#include <stdio.h>

int main() {
    int a, b, gcd, i;

    printf("Enter first number: ");
    scanf("%d", &a);

    printf("Enter second number: ");
    scanf("%d", &b)
    int min = (a < b) ? a : 
    for (i = min; i >= 1; i--) {
        if (a % i == 0 && b % i == 0) {
            gcd = i;
            break;  
        }
    }

    printf("GCD of %d and %d is %d\n", a, b, gcd);

    return 0;
}
