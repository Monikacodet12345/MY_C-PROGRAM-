#include <stdio.h>
int swap(int*a,int*b){
    int temp=*a;
    *a=*b;
    *b=temp;
}

int main() {
    int b1,b2;
    printf("enter b for a1& a2:");
    scanf("%d %d",&b1,&b2);
    printf ("\n before swapping:\n");
    printf("a1,a2 b:%d %d\n",b1,b2);
    swap(&b1,&b2);
    printf("\n after swap:\n");
    printf("a1,a2 b:%d %d\n",b1,b2);

    return 0;
}
