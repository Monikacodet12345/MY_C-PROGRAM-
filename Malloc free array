include <stdio.h>
#include<stdlib.h>

int main() {
  int *arr=(int*)malloc(5*sizeof(int));
  for(int i=0;i<5;i++){
    printf("enter elements%d:\n",i+1);
scanf("%d",&arr[i]);
printf("array elements%d:\n");
 for(int i=0;i<5;i++){
     printf("%d",arr[i]);
 }
 free(arr);
 for(int i=0;i<5;i++){
     printf("%d",arr[i]);
 }
 
    return 0;
}
}
